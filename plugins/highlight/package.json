{
  "name": "@serlo-org/ory-editor-plugins-highlight",
  "version": "0.0.0",
  "bugs": {
    "url": "https://github.com/serlo-org/ory-editor-plugins/issues"
  },
  "author": "@schul-cloud",
  "contributors": ["@Trunken", "@FWirtz"],
  "license": "Apache-2.0",
  "main": "lib/index.js",
  "repository": "serlo-org/ory-editor-plugins",
  "scripts": {
    "build": "npm-run-all --parallel build:lib build:css",
    "build:watch": "npm-run-all --parallel build:lib:watch build:css:watch",
    "build:lib":
      "babel ./src --source-maps --ignore test.js --out-dir ./lib --presets=react",
    "build:lib:watch": "npm run build:lib -- --watch",
    "build:css":
      "postcss -c ../../../postcss.config.js --dir lib/ src/index.css",
    "build:css:watch": "npm run build:css -- -w",
    "prebuild": "npm run clean",
    "clean": "rimraf \"lib/*\""
  },
  "engines": {
    "node": ">=6.0.0",
    "npm": "~3.8.0"
  },
  "dependencies": {
    "material-ui": "^0.16.7",
    "material-ui-icons": "^1.0.0-beta.17",
    "react-syntax-highlighter": "^6.1.1",
    "uuid": "^3.1.0"
  },
  "devDependencies": {},
  "peerDependencies": {
    "react": "^16.4.2",
    "react-dom": "^16.3.1"
  },
  "publishConfig": {
    "access": "public"
  }
}
